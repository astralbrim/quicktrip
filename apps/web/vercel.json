{
  "version": 2,
  "buildCommand": "cd ../.. && pnpm install && pnpm build --filter=@quicktrip/web",
  "installCommand": "echo 'Skipping default install'",
  "builds": [
    {
      "src": "next.config.js",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}